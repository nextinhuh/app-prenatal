{
  "expo": {
    "name": "App Cegonha",
    "slug": "app-cegonha",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-200.png",
      "resizeMode": "contain",
      "backgroundColor": "#F54F51"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "br.com.alvaroneto.cegonha",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json"
    }
  },
  "jsEngine": "hermes",
  "ios": {
    "jsEngine": "jsc"
  }
}
